l1=[1,2,3]
l2=["Apple","BlueBerry","Cherry"]

 # Way 1:
print(l1+l2)  # [1, 2, 3, 'Apple', 'BlueBerry', 'Cherry']

# Way 2;
for x in l2:
    l1.append(x) 

print(l1)  # [1, 2, 3, 'Apple', 'BlueBerry', 'Cherry']

# Way 3:
l1.extend(l2)
print(l1)  # [1, 2, 3, 'Apple', 'BlueBerry', 'Cherry', 'Apple', 'BlueBerry', 'Cherry']
