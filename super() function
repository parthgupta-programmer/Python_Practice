#Code 2
class Employee:
    def __init__(self,name):
        self.name=name
class Manager(Employee):
    def __init__(self,name,dep):
        super().__init__(name)
        self.dep=dep
e1=Manager("Jos Buttler","HR")
print(e1.name)
print(e1.dep)
