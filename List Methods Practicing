l=[i*i for i in range(11) if i%2==0]
print(l)
l.append(144)
print(l)
l.extend([256,384,400])
print(l)
l1=[1,4,3,5,2,9,4,7,8,6,10,8,4,4]
l1.sort()
print(l1)
print(l1.count(4))
l1.sort(reverse=True)
print(l1)
l.reverse()
l_tem=l.copy()
print(l)
print(l_tem)
l_tem[4]=None
print(l_tem)
l_tem.pop(4)
print(l_tem)
l1.remove(4)
print(l1)
l1.clear()
print(l1)
print(l.index(4))
print(len(l))
